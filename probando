gifshot.createGIF({
    gifWidth: 200,
    gifHeight: 200,
    images: [
        'C:\Users\Lupiluna\Pictures\5 FOTOS IMAGENES\1\salle dia niño.jpeg',
        ':\Users\Lupiluna\Pictures\5 FOTOS IMAGENES\1\salle dia niño.jpeg',
        
    ],
    interval: 2,
    numFrames: 10,
    frameDuration: 1,
    fontWeight: 'normal',
    fontSize: '16px',
    fontFamily: 'sans-serif',
    fontColor: '#800000',
    textAlign: 'center',
    textBaseline: 'bottom',
    sampleInterval: 10,
    numWorkers: 2
}, function (obj) {
    if (!obj.error) {
        var image = obj.image, animatedImage = document.createElement('img');
        animatedImage.src = image;
        document.body.appendChild(animatedImage);
    }
});
